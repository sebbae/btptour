{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "globals": {
        "SELECT": true,
        "INSERT": true,
        "UPDATE": true,
        "DELETE": true,
        "CREATE": true,
        "DROP": true,
        "cds": true
    },
    "rules": {
        "max-len": "off",
        "no-console": "off",
        "require-atomic-updates": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "consistent-return": "off",
        "global-require": "off",
        "prefer-arrow-callback": "off",
        "func-names": "off"
    }
}