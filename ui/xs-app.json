{
	"routes": [
		{
			"source": "^/preview/(.*)$",
			"target": "/$fiori-preview/$1",
			"destination": "BTPTOUR_BACKEND",
			"authenticationType": "none"
		},
		{
			"source": "^/apps/people/webapp/info/(.*)$",
			"target": "/info/$1",
			"destination": "BTPTOUR_BACKEND",
			"authenticationType": "none"
		},
		{
			"source": "^/api-docs/(.*)$",
			"target": "/api-docs/$1",
			"destination": "BTPTOUR_BACKEND",
			"authenticationType": "none"
		},
		{
			"source": "^/websocket$",
			"target": "/",
			"destination": "BTPTOUR_BACKEND",
			"authenticationType": "none"
		},
		{
			"source": "^/resources/(.*)$",
			"target": "/resources/$1",
			"destination": "ui5"
		}
	]
}
